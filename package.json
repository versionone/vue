{
  "name": "@versionone/vue",
  "version": "0.0.1",
  "description": "Open-source and community supported collection of common UI components built with React. As an open-sourced and community supported project, VersionOne Vue is not formally supported by VersionOne.",
  "repository": {
    "type": "git",
    "url": "https://github.com/versionone/vue.git"
  },
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=release node_modules/.bin/babel src --ignore Stories.js,*.spec.js,_meta.js,examples/*.* --out-dir ./",
    "specs": "cross-env NODE_ENV=test node_modules/.bin/mocha --ui tdd --require specSetup.js --compilers js:babel-core/register src/**/*.spec.js --recursive",
    "test": "npm run lint && npm run specs",
    "lint": "npm run lint:src & npm run lint:specs",
    "lint:src": "eslint src/**/*.js --fix --ignore-pattern *.spec.js --config eslint.src.js",
    "lint:specs": "eslint src/**/*.spec.js --fix --config eslint.specs.js",
    "lint:watch": "npm run lint:src:watch & npm run lint:specs:watch",
    "lint:src:watch": "esw --watch --color src/**/*.js --ignore-pattern *.spec.js --config eslint.src.js",
    "lint:specs:watch": "esw --watch --color src/**/*.spec.js --config eslint.specs.js",
    "start": "cd docs && npm start && cd ../",
    "start:dev": "start-storybook -p 9001",
    "preversion": "npm test",
    "publish": "npm run test && npm run build && npm publish --registry http://artifactory.dev.corp.versionone.net/artifactory/api/npm/local-npm",
    "postpublish": "npm run clean && npm run publish:docs",
    "publish:docs": "cd docs && npm run gh-pages:build -p && cd ../",
    "verify": "node scripts/verify",
    "preinstall": "npm run verify",
    "postinstall": "cd docs && npm install && cd ../"
  },
  "keywords": [
    "react",
    "components",
    "VersionOne",
    "UI"
  ],
  "author": "Andrew Smith <andrew@andrew.codes>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3",
      "stage-2",
      "stage-1"
    ],
    "env": {
      "release": {
        "plugins": [
          [
            "transform-react-remove-prop-types",
            {
              "mode": "wrap"
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.0.3",
    "@kadira/storybook": "^1.33.0",
    "@kadira/storybook-deployer": "^1.0.0",
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.10.2",
    "eslint-config-andrew-codes": "^3.1.1",
    "eslint-config-andrew-codes-react": "^1.1.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^2.1.14",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-storybook-addon-add-stories-group": "^0.1.0",
    "rimraf": "^2.5.2",
    "simulant": "^0.2.2",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "wallaby-webpack": "0.0.22",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "radium": "^0.18.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "invariant": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "radium": "^0.18.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-event-listener": "^0.4.0",
    "warning": "^3.0.0"
  }
}
