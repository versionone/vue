{
  "name": "material-ui-docs",
  "version": "0.16.0-alpha",
  "description": "Documentation site for material-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "engines": {
    "node": ">=4"
  },
  "private": true,
  "scripts": {
    "start": "npm run browser:development",
    "browser:development": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --inline",
    "browser:build": "NODE_ENV=docs-production webpack --config webpack-production.config.js --progress --colors --profile",
    "browser:prd": "NODE_ENV=docs-production webpack-dev-server --config webpack-production.config.js --progress --colors",
    "gh-pages:build": "node ./gh-pages-build.js"
  },
  "dependencies": {
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "lunr": "^0.7.1",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "react-title-component": "^1.0.1",
    "simple-assign": "^0.1.0",
    "uniqueid": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "doctrine": "^1.1.0",
    "eslint": "^3.0.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^5.0.1",
    "highlight.js": "^9.5.0",
    "history": "^2.0.0",
    "intl": "^1.2.3",
    "intl-locales-supported": "^1.0.0",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "raw-loader": "^0.5.1",
    "react-addons-perf": "^15.2.1",
    "react-docgen": "^2.9.1",
    "react-hot-loader": "^1.3.0",
    "react-motion": "^0.4.2",
    "react-router": "^2.5.2",
    "react-swipeable-views": "^0.6.3",
    "recast": "^0.11.10",
    "style-loader": "0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.10.1"
  }
